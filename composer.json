{
    "name": "madj2k/t3-core-extended",
    "type": "typo3-cms-extension",
    "description": "Adds some basic features to your TYPO3 installation",
    "homepage": "https://www.steffenkroggel.de",
    "license": ["GPL-2.0+"],
    "keywords": ["TYPO3 CMS", "Layout", "Google Sitemap", "SEO", "ViewHelpers", "Fluid", "Frontend Simulation", "General Tools", "Cache", "Asset Files", "Routing", "Media Sources"],
    "support": {
        "email": "developer@steffenkroggel.de"
    },
    "authors": [
        {
            "name": "Steffen Kroggel",
            "email": "developer@steffenkroggel.de",
            "homepage": "http://www.steffenkroggel.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
		"typo3/cms-core": "~9.5.0 || ~10.4.0",
		"typo3/cms-seo": "~9.5.0 || ~10.4.0",
		"typo3/cms-filemetadata": "~9.5.0 || ~10.4.0",
		"calien/persisted-sanitized-routing" : "^1.0.4",
		"league/csv": "^9.5"
	},
	"suggest": {
		"sjbr/sr-freecap": "~2.5.0"
	},
    "autoload": {
        "psr-4": {
            "Madj2k\\CoreExtended\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "core_extended"
        }
    }
}
